<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="UnusedAttribute">

    <font
        android:font="@font/firasans_regular"
        android:fontStyle="normal"
        android:fontWeight="400"

        app:font="@font/firasans_regular"
        app:fontStyle="normal"
        app:fontWeight="400" />

    <font
        android:font="@font/firasans_italic"
        android:fontStyle="italic"
        android:fontWeight="400"

        app:font="@font/firasans_italic"
        app:fontStyle="italic"
        app:fontWeight="400" />

    <font
        android:font="@font/firasans_medium"
        android:fontStyle="normal"
        android:fontWeight="600"

        app:font="@font/firasans_medium"
        app:fontStyle="normal"
        app:fontWeight="600" />

    <font
        android:font="@font/firasans_mediumitalic"
        android:fontStyle="italic"
        android:fontWeight="600"

        app:font="@font/firasans_mediumitalic"
        app:fontStyle="italic"
        app:fontWeight="600" />

    <font
        android:font="@font/firasans_bold"
        android:fontStyle="normal"
        android:fontWeight="700"

        app:font="@font/firasans_bold"
        app:fontStyle="normal"
        app:fontWeight="700" />

    <font
        android:font="@font/firasans_bolditalic"
        android:fontStyle="italic"
        android:fontWeight="700"

        app:font="@font/firasans_bolditalic"
        app:fontStyle="italic"
        app:fontWeight="700" />

</font-family>